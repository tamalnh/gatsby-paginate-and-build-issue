{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM", "ES2017"],
    "module": "CommonJS",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2017",
    "noImplicitAny": false
  },
  "include": ["./src/**/*", "next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "public", ".cache"]
}
